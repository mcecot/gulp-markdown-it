{
  "name": "gulp-markdown-it",
  "version": "0.1.2",
  "description": "A plugin for gulp to render markdown with markdown-it",
  "keywords": [
    "gulp",
    "gulpplugin",
    "markdown",
    "markdown-it"
  ],
  "repository": "mcecot/gulp-markdown-it",
  "author": {
    "name": "markus cecot",
    "email": "mail@cecot.de",
    "url": ""
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "prepublish": "gulp coffee --require coffee-script/register",
    "test": "coffeelint gulpfile.coffee index.coffee test -f ./coffeelint.json && istanbul test _mocha --report lcov --report html -- ./test/*.coffee --require coffee-script/register --reporter spec",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {
    "gulp-util": "^3.0.0",
    "through2": "^2.0.3",
    "underscore": "^1.8.2",
    "markdown-it": "^8.x"
  },
  "devDependencies": {
    "coffee-script": "^1.7.1",
    "coffeelint": "^1.4.0",
    "coveralls": "^2.8.0",
    "del": "^2.2.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "gulp": "^3.5.2",
    "gulp-coffee": "^2.1.2",
    "gulp-util": "^3.0.4",
    "should": "^11.2.1",
    "gulp-bump": "^2.7.0",
    "gulp-git": "^2.1.0",
    "gulp-tag-version": "^1.2.1",
    "markdown-it-checkbox": "^1.1.0"
  },
  "engines": {
    "node": ">=4.2.0",
    "npm": ">=1.3.7"
  },
  "license": "MIT"
}
